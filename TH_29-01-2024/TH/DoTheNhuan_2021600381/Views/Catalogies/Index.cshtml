@model IEnumerable<DoTheNhuan_2021600381.Models.Catalogy>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Catalogy</h2>

<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CatalogyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CatalogyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.CatalogyID }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.CatalogyID }) |*@
            @Html.ActionLink("Xóa", "Delete", new { id=item.CatalogyID })
        </td>
    </tr>
}

</table>
